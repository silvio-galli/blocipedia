<table class="table table-bordered table-hover">
  <tr class="bg-info">
    <th>wiki title</th>
    <th>created at</th>
    <th></th>
    <th></th>
  </tr>
  <% wikis.each do |wiki| %>
    <% if wiki.user_id == current_user.id %>
      <tr class="active">
        <td><%= link_to wiki.title, wiki_path(wiki.id), title: "Read this wiki." %></td>
        <td><small><%= wiki.created_at.strftime("%B %d, %Y") %></small></td>
        <td><%= link_to "edit", edit_wiki_path(wiki.id), title: "Edit this wiki" %></td>
        <td><%= link_to "delete", wiki, method: :delete, title: "Delete this wiki", data: { confirm: 'Are you sure you want to delete this wiki?' } %></td>
      </tr>
    <% end %>
  <% end %>
</table>
